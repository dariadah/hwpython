a = float(input('Введиие 1-е число'))
b = float(input('Введите 2-е число'))

addition = a + b
substraction = a - b #вычитание
division = a/b #деление
multiplication = a*b #умножение
dev_mod = a%b #деление по модулю
st = a**b #возведение в степень
tselochislennoe_dilenie = a//b #целочисленное диление
konkatenatsiya = str(a) + str(b)

print('Результат суммы: {} Результат вычитания: {} '.format(addition, substraction))
print('Результат деления: {} Результат деления по модулю: {}'.format(division, dev_mod))
print('Результат возведения в степень: {} Результат умножения: {} '.format(st, multiplication))
print('Результат целочисленного диления: {} Результат конкатенация: {}'.format(tselochislennoe_dilenie, konkatenatsiya))

print('----------')

print("1-е число меньше 2-го: ", a < b)       # меньше
print("2-е число больше 1-го: ", b > a)       # больше
print("2-е число меньше или равно 1-му: ", a <= b)      # меньше или равно
print("2-е число больше или равно 1-му: ", b >= a)      # больше или равно
print('-------')
print("Двойное сравнение 2-х чисел с 100: ", a < 100 < b)   # двойное сравнение
print("1-е число равное 2-му: ", a == b)      # равенство
print("1-е число не равно 2-му: ", a != b)      # неравенство
print('-------')
print("Идентичность объектов в памяти: ", a is b)      # идентичность объектов в памяти
print("Проверка объектов на различие: ", a is not b)  # a и b – разные объекты (хотя значения их могуть быть равны)


